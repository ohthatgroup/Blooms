const CATEGORIES = […new Set(PRODUCTS.map(p => p.category))];
</script>

<script type="text/babel">
const { useState, useMemo, useCallback, useRef } = React;

function App() {
  const [quantities, setQuantities] = useState({});
  const [search, setSearch] = useState('');
  const [customerName, setCustomerName] = useState('');
  const [activeTab, setActiveTab] = useState('ALL');
  const [showReview, setShowReview] = useState(false);
  const [toast, setToast] = useState('');

  const showToast = (msg) => {
    setToast(msg);
    setTimeout(() => setToast(''), 2500);
  };

  const setQty = useCallback((sku, val) => {
    const num = Math.max(0, parseInt(val) || 0);
    setQuantities(prev => {
      const next = { ...prev };
      if (num === 0) delete next[sku];
      else next[sku] = num;
      return next;
    });
  }, []);

  const filteredProducts = useMemo(() => {
    let items = PRODUCTS;
    if (activeTab !== 'ALL') {
      items = items.filter(p => p.category === activeTab);
    }
    if (search.trim()) {
      const q = search.toLowerCase();
      items = items.filter(p =>
        p.name.toLowerCase().includes(q) ||
        p.sku.toLowerCase().includes(q) ||
        p.upc.includes(q)
      );
    }
    return items;
  }, [activeTab, search]);

  const groupedProducts = useMemo(() => {
    const groups = {};
    filteredProducts.forEach(p => {
      if (!groups[p.category]) groups[p.category] = [];
      groups[p.category].push(p);
    });
    return groups;
  }, [filteredProducts]);

  const orderItems = useMemo(() => {
    return Object.entries(quantities)
      .map(([sku, qty]) => {
        const product = PRODUCTS.find(p => p.sku === sku);
        return product ? { ...product, qty } : null;
      })
      .filter(Boolean)
      .sort((a, b) => a.category.localeCompare(b.category) || a.name.localeCompare(b.name));
  }, [quantities]);

  const totalItems = orderItems.reduce((sum, item) => sum + item.qty, 0);
  const totalSkus = orderItems.length;

  const downloadCSV = () => {
    if (orderItems.length === 0) return;
    const name = customerName.trim() || 'Customer';
    const date = new Date().toLocaleDateString('en-US');
    let csv = 'Customer,Date,SKU,Product,UPC,Pack,Qty\n';
    orderItems.forEach(item => {
      const prodName = item.name.replace(/,/g, ' ');
      csv += `"${name}","${date}","${item.sku}","${prodName}","${item.upc}","${item.pack}",${item.qty}\n`;
    });
    const blob = new Blob([csv], { type: 'text/csv' });
    const url = URL.createObjectURL(blob);
    const a = document.createElement('a');
    a.href = url;
    a.download = `Blooms_Order_${name.replace(/\s+/g, '_')}_${new Date().toISOString().slice(0,10)}.csv`;
    a.click();
    URL.revokeObjectURL(url);
    showToast('\u2705 CSV Downloaded!');
  };

  const clearOrder = () => {
    if (confirm('Clear entire order?')) {
      setQuantities({});
      setShowReview(false);
      showToast('Order cleared');
    }
  };

  return (
    <>
      <div className="header">
        <h1>Bloom Packaging Corp.</h1>
        <p>Digital Order Form \u2022 {PRODUCTS.length} Products</p>
      </div>

      <div className="customer-bar">
        <input
          type="text"
          placeholder="Customer / Store Name"
          value={customerName}
          onChange={e => setCustomerName(e.target.value)}
        />
      </div>

      <div className="search-bar">
        <input
          type="text"
          placeholder="Search by name, SKU, or UPC..."
          value={search}
          onChange={e => setSearch(e.target.value)}
        />
      </div>

      <div className="tabs">
        <div
          className={`tab ${activeTab === 'ALL' ? 'active' : ''}`}
          onClick={() => setActiveTab('ALL')}
        >
          All ({PRODUCTS.length})
        </div>
        {CATEGORIES.map(cat => (
          <div
            key={cat}
            className={`tab ${activeTab === cat ? 'active' : ''}`}
            onClick={() => setActiveTab(cat)}
          >
            {cat}
          </div>
        ))}
      </div>

      <div style={{ padding: '8px 0' }}>
        {Object.entries(groupedProducts).map(([cat, items]) => (
          <div key={cat} className="category-section">
            <div className="category-header">
              {cat}
              <span className="count">{items.length}</span>
            </div>
            {items.map(p => (
              <ProductRow
                key={p.sku}
                product={p}
                qty={quantities[p.sku] || 0}
                setQty={setQty}
              />
            ))}
          </div>
        ))}
        {filteredProducts.length === 0 && (
          <div className="empty-state">No products found</div>
        )}
      </div>

      <div className="bottom-bar">
        {totalSkus > 0 && (
          <div className="item-count">{totalSkus} items \u2022 {totalItems} cases</div>
        )}
        {totalSkus > 0 && (
          <button className="btn-review" onClick={() => setShowReview(true)}>
            Review
          </button>
        )}
        <button
          className="btn-csv"
          onClick={downloadCSV}
          disabled={totalSkus === 0}
        >
          {totalSkus === 0 ? 'Add items to order' : 'Download CSV'}
        </button>
      </div>

      {showReview && (
        <div className="modal-overlay" onClick={() => setShowReview(false)}>
          <div className="modal" onClick={e => e.stopPropagation()}>
            <h2>Order Review {customerName ? `\u2014 ${customerName}` : ''}</h2>
            <p style={{ fontSize: 14, color: '#666', marginBottom: 12 }}>
              {totalSkus} products \u2022 {totalItems} total cases
            </p>
            {orderItems.map(item => (
              <div key={item.sku} className="modal-item">
                {item.img && <img src={`data:image/jpeg;base64,${item.img}`} alt="" />}
                <div style={{ flex: 1, minWidth: 0 }}>
                  <span style={{ fontWeight: 600 }}>{item.sku}</span>
                  {' '}\u2014 {item.name}
                  <br/>
                  <span style={{ fontSize: 12, color: '#999' }}>{item.pack}</span>
                </div>
                <div className="qty">{item.qty}</div>
              </div>
            ))}
            <button className="modal-close" onClick={downloadCSV}>
              Download CSV
            </button>
            <button className="modal-clear" onClick={clearOrder}>
              Clear Entire Order
            </button>
          </div>
        </div>
      )}

      {toast && <div className="toast">{toast}</div>}
    </>
  );
}

const ProductRow = React.memo(({ product, qty, setQty }) => {
  return (
    <div className={`product-row ${qty > 0 ? 'has-qty' : ''}`}>
      {product.img ? (
        <img className="product-img" src={`data:image/jpeg;base64,${product.img}`} alt="" loading="lazy" />
      ) : (
        <div className="product-img" />
      )}
      <div className="product-info">
        <div className="product-sku">{product.sku}</div>
        <div className="product-name">{product.name}</div>
        <div className="product-pack">{product.pack}{product.upc ? ` \u2022 ${product.upc}` : ''}</div>
      </div>
      <div className="qty-controls">
        <button
          className="qty-btn minus"
          onClick={() => setQty(product.sku, qty - 1)}
        >\u2212</button>
        <input
          className="qty-input"
          type="number"
          min="0"
          value={qty || ''}
          placeholder="0"
          onChange={e => setQty(product.sku, e.target.value)}
        />
        <button
          className="qty-btn plus"
          onClick={() => setQty(product.sku, qty + 1)}
        >+</button>
      </div>
    </div>
  );
});

ReactDOM.render(<App />, document.getElementById('root'));
</script>

</body>
</html>